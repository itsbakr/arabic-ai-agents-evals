# API Keys Configuration
# Copy this content to .env file and add your actual API keys

# Google Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic Claude API Key
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Weave Inference API Key (for Qwen model)
# Get from your Weave dashboard
WEAVE_API_KEY=your_weave_api_key_here
WEAVE_BASE_URL=https://your-weave-endpoint.com/v1

# Weave Tracing Configuration (Weights & Biases)
# Get WANDB_API_KEY from: https://wandb.ai/settings
# Set ENABLE_WEAVE_TRACING to 'false' to disable tracing
ENABLE_WEAVE_TRACING=true
WEAVE_PROJECT_NAME=arabic-cs-agents-eval
WANDB_API_KEY=your_wandb_api_key_here
WANDB_ENTITY=your_wandb_username

# Supabase Configuration (optional - for database storage)
# Get from: https://supabase.com/dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Storage Configuration
# Options: 'csv', 'supabase', or 'both'
STORAGE_MODE=csv

